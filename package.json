{
  "name": "sketch-resize-layers",
  "version": "0.1.1",
  "description": "A Sketch plugin to ease layer resizing",
  "keywords": [
    "plugin",
    "sketch",
    "sketch-plugin-helper"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": "yuanqing/sketch-resize-layers",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "sketch build",
    "fix": "sketch lint --fix",
    "handler": "sketch handler",
    "lint": "sketch lint",
    "script": "sketch script",
    "symlink": "sketch symlink",
    "test": "sketch test",
    "unlink": "sketch symlink --delete",
    "version": "sketch version",
    "watch": "sketch build --watch"
  },
  "devDependencies": {
    "sketch-plugin-helper": "*"
  },
  "sketch-plugin-helper": {
    "name": "Resize Layers",
    "menu": [
      {
        "Resize Up": [
          {
            "handler": "resize-up/top-1",
            "name": "Top 1"
          },
          {
            "handler": "resize-up/top-10",
            "name": "Top 10"
          },
          "-",
          {
            "handler": "resize-up/bottom-1",
            "name": "Bottom 1"
          },
          {
            "handler": "resize-up/bottom-10",
            "name": "Bottom 10"
          },
          "-",
          {
            "handler": "resize-up/left-1",
            "name": "Left 1"
          },
          {
            "handler": "resize-up/left-10",
            "name": "Left 10"
          },
          "-",
          {
            "handler": "resize-up/right-1",
            "name": "Right 1"
          },
          {
            "handler": "resize-up/right-10",
            "name": "Right 10"
          }
        ]
      },
      {
        "Resize Down": [
          {
            "handler": "resize-down/top-1",
            "name": "Top 1"
          },
          {
            "handler": "resize-down/top-10",
            "name": "Top 10"
          },
          "-",
          {
            "handler": "resize-down/bottom-1",
            "name": "Bottom 1"
          },
          {
            "handler": "resize-down/bottom-10",
            "name": "Bottom 10"
          },
          "-",
          {
            "handler": "resize-down/left-1",
            "name": "Left 1"
          },
          {
            "handler": "resize-down/left-10",
            "name": "Left 10"
          },
          "-",
          {
            "handler": "resize-down/right-1",
            "name": "Right 1"
          },
          {
            "handler": "resize-down/right-10",
            "name": "Right 10"
          }
        ]
      }
    ]
  }
}
